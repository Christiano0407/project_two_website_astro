// === Static site Generation (SSG) ===

export async function getStaticPaths() {
  const pages = [
  {
    slug: '/', 
    name: 'Home', 
    title: 'Home Page Website', 
    description: 'Welcome to the Home Page'
  },
  {
    slug: '/Clothes/', 
    name: 'Clothes',
    title: 'Page To Clothes', 
    description: 'Welcome To the Page Clothes'
  }, 
  {
    slug: '/Contact/',
    name: 'Contact', 
    title: 'Page To Contact', 
    description: 'Welcome To the Page Contact'
  }, 
  {
    slug: '/Podcast/', 
    name: 'Podcast',
    title: 'Page To Podcast', 
    description: 'Welcome To the Page Podcast'
  }, 
  {
    slug: '/Blog/', 
    name: 'Blog',
    title: 'Blog Page', 
    description: 'Welcome at the Blog'
  }, 
  {
    slug: '/Products/', 
    name: 'Products',
    title: 'Page Of The Products', 
    description: 'Welcome To the Page Products'
  }, 
]; 

  return pages.map(( { slug, name, title, description } ) => ({
     params: { slug }, 
     props: { name, title, description }
  })); 

}

